<div class="row home_section">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">

            </div>
            <!-- /.box-header -->
            <div class="box-body">
                <table id="table_assignment_history" class="table table-bordered table-striped">
                    <thead>
                    <tr>
                        <th class="center">배정일</th>
                        <th class="center">교육과정명</th>
                        <th class="center">대상자</th>
                        <th class="center">시작일</th>
                        <th class="center">종료일</th>
                        <th class="center">배정자</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each list}}
                        <tr>
                            <td class="center">{{time created_dt}}</td>
                            <td class="center">{{name}}</td>
                            <td class="center">
                                {{#if (isEmpty target)}}
                                    -
                                {{else}}
                                    {{target}}
                                {{/if}}
                            </td>
                            <td class="center">{{time start_dt}}</td>
                            <td class="center">{{time end_dt}}</td>
                            <td class="center">{{admin}}</td>
                        </tr>
                    {{/each}}
                    </tbody>
                    <tfoot>
                    <tr>
                        <th class="center">배정일</th>
                        <th class="center">교육과정명</th>
                        <th class="center">대상자</th>
                        <th class="center">시작일</th>
                        <th class="center">종료일</th>
                        <th class="center">배정자</th>
                    </tr>
                    </tfoot>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
    <!-- /.col -->
</div>
<!-- /.row -->

