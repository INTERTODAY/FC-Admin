
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="/static/stylesheets/bootstrap.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/font-awesome.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/ionicons.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/AdminLTE.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/skin-green-light.min.css">
    <link rel="stylesheet" href="/static/stylesheets/datepicker3.css" />
    <link rel="stylesheet" href="/static/stylesheets/daterangepicker.css" />
    <link rel="stylesheet" href="/static/stylesheets/select2.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/dataTables.bootstrap.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/buttons.bootstrap.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/responsive.bootstrap.min.css" />
    <link rel="stylesheet" href="/static/stylesheets/bootstrap-datetimepicker.min.css" />
    <link rel='stylesheet' href="/static/stylesheets/styles.css" />

    <title>{{title}}</title>
</head>

<body class="hold-transition skin-green-light sidebar-mini {{#if (isEquals current_path 'Login')}}login-page{{/if}}">
    {{#if (isEquals current_path 'Login')}}
    {{{body}}}
    {{else if (isEquals current_path 'winpop')}}
    {{{body}}}
    {{else}}
    <div class="wrapper">
        {{> header}}
        {{> left_nav}}
        <div class="content-wrapper">
            {{> page_header}}
            <section class="content">{{{body}}}</section>
        </div>
    </div>
    {{> modal_change_password}}
    {{> footer}}
    {{/if}}

    <script src="/static/vendor/require.2.3.2.js"></script>
    <script src="/javascripts/app.js"></script>

    {{#if (isEquals current_path 'Login')}}<script src="/javascripts/login.js"></script>{{/if}}
    {{#if (isEquals current_path 'Dashboard')}}<script src="/javascripts/dashboard.js"></script>{{/if}}
    {{#if (isEquals current_path 'Employee')}}<script src="/javascripts/employee.js"></script>{{/if}}
    {{#if (isEquals current_path 'Education')}}<script src="/javascripts/education.js"></script>{{/if}}
    {{#if (isEquals current_path 'EducationDetails')}}<script src="/javascripts/education_details.js"></script>{{/if}}
    {{#if (isEquals current_path 'Assignment')}}<script src="/javascripts/assignment.js"></script>{{/if}}
    {{#if (isEquals current_path 'AssignmentDetails')}}<script src="/javascripts/assignment_details.js"></script>{{/if}}
    {{#if (isEquals current_path 'Assignment_history')}}<script src="/javascripts/assignment_history.js"></script>{{/if}}
    {{#if (isEquals current_path 'Course')}}<script src="/javascripts/course.js"></script>{{/if}}
    {{#if (isEquals current_path 'Achievement')}}<script src="/javascripts/achievement.js"></script>{{/if}}
    {{#if (isEquals current_path 'AchievementDetails')}}<script src="/javascripts/achievement_details.js"></script>{{/if}}
    {{#if (isEquals current_path 'CourseDetails')}}<script src="/javascripts/course_details.js"></script>{{/if}}
    {{#if (isEquals current_path 'Administrator')}}<script src="/javascripts/administrator.js"></script>{{/if}}
    {{#if (isEquals module_type 'create_video')}}<script src="/javascripts/winpops/win_create_video.js"></script>{{/if}}
    {{#if (isEquals module_type 'modify_video')}}<script src="/javascripts/winpops/win_modify_video.js"></script>{{/if}}
    {{#if (isEquals module_type 'create_quiz')}}<script src="/javascripts/winpops/win_create_quiz.js"></script>{{/if}}
    {{#if (isEquals module_type 'modify_quiz')}}<script src="/javascripts/winpops/win_modify_quiz.js"></script>{{/if}}
    {{#if (isEquals module_type 'show_quiz')}}<script src="/javascripts/winpops/win_show_quiz.js"></script>{{/if}}

</body>
</html>