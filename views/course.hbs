<div class="row home_section">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header">
        <div class="pull-right">
          <button class="btn btn-primary" data-toggle="modal" data-target="#createTeacher">
            <i class="fa fa-pencil-square-o"></i>
            강사 등록
          </button>
          <button class="btn btn-default" data-toggle="modal" data-target="#registerCourse">
            <i class="fa fa-pencil-square-o"></i>
            강의 등록
          </button>
        </div>
      </div>
      <!-- /.box-header -->
      <div class="box-body table-responsive">
        <table id="table_course" class="table no-margin table-bordered table-striped" width="100%">
          <thead>
            <tr>
              <th class="center">생성일</th>
              <th class="center">강의명</th>
              <th class="center">강사명</th>
              <th class="center">생성자</th>
              <!--<th class="center">Action</th>-->
            </tr>
          </thead>
          <tbody>
            {{#each list}}
            <tr>
              <td class="center">{{time created_dt}}</td>
              <td class=""><a href="/course/details?id={{course_id}}" class="btn-link">{{name}}</a></td>
              <td class="center">{{teacher}}</td>
              <td class="center">{{creator}}</td>
              <!--<td class="center">
                                <a href="/course/details?id={{course_id}}" class="btn btn-default btn-sm">
                                    <i class="fa fa-eye"></i>&nbsp;
                                    상세보기
                                </a>
                            </td>-->
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      <!-- /.box-body -->
    </div>
    <!-- /.box -->
  </div>
  <!-- /.col -->
</div>
<!-- /.row -->

{{> modal_view_teacher}} {{> modal_register_teacher}} {{> modal_register_course}}